//AcWing
#include<iostream>

using namespace std;

int n;

int main()
{
    scanf("%d", &n);
    
    int x;
    int res = 0;
    for (int i = 0; i < n; ++i) {
        scanf("%d", &x);
        if (i % 2 == 0) {
            res ^= x;
        }
    }
    if (res) puts("Yes");
    else puts("No");
    
    return 0;
}
