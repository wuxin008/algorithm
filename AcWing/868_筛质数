#include<iostream>

using namespace std;

const int N = 1000100;

int n;
int primes[N], cnt;
int st[N];

void get_primes(int n)
{
    for (int i = 2; i <= n; ++i) {
        if (!st[i]) {
            primes[cnt++] = i;
            for (int j = i + i; j <= n; j += i) st[j] = 1;
        }
    }
}

int main()
{
    scanf("%d", &n);
    
    get_primes(n);
    
    printf("%d\n", cnt);
    
    return 0;
}
