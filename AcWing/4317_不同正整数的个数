//AcWing
#include<iostream>

using namespace std;

const int N = 100010;

int n;
int a[N];

int main()
{
    cin >> n;
    
    int x;
    int res = 0;
    while (n--) {
        cin >> x;
        if (x > 0 && !a[x]) {
            a[x]++;
            res++;
        }
    }
    
    cout << res << endl;
    
    return 0;
}
